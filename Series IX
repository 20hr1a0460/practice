import java.util.Scanner;
public class Series9
{
    public static void main(String[] args)
    {
      Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      long s=2l;
      System.out.print(s + " ");
      for(int i=1;i<n;i++){
          s=(s*s)-1;
          System.out.print(s + " ");//Write your code
    }
    }
}
