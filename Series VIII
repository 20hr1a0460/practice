import java.util.*;
public class Series8
{
    public static void main(String[] args)
    {
     Scanner sc=new Scanner(System.in);
     long a=sc.nextLong();
     long m=4;
       System.out.println(m); 
     for(int i=1;i<a;i++)
     {
       m=m+(i*i);
         System.out.println(m);
     }
    }
}
